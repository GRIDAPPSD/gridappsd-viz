{
  "name": "react-json-viewer",
  "version": "1.7.1",
  "description": "react component for json viewing in tabular format",
  "main": "dist/ReactJSONViewer.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "precommit": "npm run eslint",
    "devserver": "webpack-dev-server --config webpack.dev.js",
    "productionBuild": "npm run prettier && npm run build",
    "build": "webpack -p --config webpack.prod.js",
    "buildJSON": "webpack --json --config webpack.dev.js> buildStats.json",
    "prettier": "prettier --write \"src/**/*.js\"",
    "eslint": "eslint -c .eslintrc src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsisodiya/react-json-viewer.git"
  },
  "keywords": [
    "react",
    "react-component",
    "json"
  ],
  "author": "Narendra Sisodiya <narendra@narendrasisodiya.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nsisodiya/react-json-viewer/issues"
  },
  "homepage": "https://github.com/nsisodiya/react-json-viewer#readme",
  "peerDependencies": {
    "prop-types": ">=15.5.8",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.13.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.14.3",
    "prettier": "^1.9.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  }
}
